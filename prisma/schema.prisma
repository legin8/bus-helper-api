// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Route {
  title           String    @id @unique
  locations       String
  key             Int       @unique
  services        Service[]
}

model Service {
  code            String    @id @unique
  name            String
  routeTitle      String
  key 	          Int
  route           Route     @relation(fields: [routeTitle], references: [title])
}